{
  "name": "modulesapp",
  "version": "1.0.0",
  "engines": {
    "node": "12.18.4",
    "npm": "6.14.6"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server",
    "client-install": "npm install --prefix client",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build --prefix client",
    "test": "mocha ./tests/*.test.js"
  },
  "dependencies": {
    "angular": "^1.8.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "common": "^0.2.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.10",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "latest-version": "^5.1.0",
    "materialize-css": "^1.0.0-rc.2",
    "moment": "^2.27.0",
    "mongoose": "^5.9.21",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "socket-io": "^1.0.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "^5.2.0",
    "mocha": "^8.1.2",
    "supertest": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "hbs": "^4.1.1"
  }
}
